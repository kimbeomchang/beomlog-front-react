{
	"singleQuote": true,
	"printWidth": 100,
	"tabWidth": 2,
	"useTabs": true,
	"semi": true,
	"quoteProps": "as-needed",
	"jsxSingleQuote": false,
	"trailingComma": "es5",
	"arrowParens": "always",
	"bracketSpacing": true,
	"jsxBracketSameLine": false,
	"proseWrap": "preserve",
	"importOrder": [
		"^react(.*)",
		"<THIRD_PARTY_MODULES>",
		"^@stores/(.*)$",
		"^@apis/(.*)$",
		"^@utils/(.*)$",
		"^@hooks/(.*)$",
		"^@interfaces/(.*)$",
		"^@assets/(.*)$",
		"^@components/(.*)$",
		"^@pages/(.*)$",
		"@/(.*)",
		"^[./]"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
